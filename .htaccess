RewriteEngine on

RewriteRule ^bugs.html support.html [L]
RewriteRule ^contribute.html get-involved.html [L]
RewriteRule ^license.html /copyright/ [L]
RewriteRule ^octave.pdf https://docs.octave.org/octave.pdf [R=301,L]
RewriteRule ^doc$ https://docs.octave.org [R=301,L]
RewriteRule ^doc/(.*)$ https://docs.octave.org/$1 [R=301,L]
RewriteRule ^doxygen$ https://docs.octave.org/doxygen [R=301,L]
RewriteRule ^doxygen/(.*)$ https://docs.octave.org/doxygen/$1 [R=301,L]
RewriteRule ^hg/(.*)$ https://hg.octave.org/$1 [R=301,L]
RewriteRule ^hg$ https://hg.octave.org [R=301,L]
RewriteRule ^testfailure/(.*)$ https://savannah.gnu.org/bugs/$1 [R=301,L]
